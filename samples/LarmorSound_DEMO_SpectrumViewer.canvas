        "extDeps" : {},
        "code" : "require Math;
require OpenImageIO;
require Images;

dfgEntry
{ 

  image   = Image2DColor();

  UInt32 resX = 1024;
  UInt32 resY = 512;

  Integer numPixels = resX * resY;
  image.resize(resX, resY);
  
  for (UInt32 i=0;i<image.pixels.size();i++)
      image.pixels[i].set(0.0, 0.0, 0.0, 0.0);
  /*
  for (UInt32 y=0;y<resY;y++)
  {
    UInt32 offset = y * resX;
    for (UInt32 x=0;x<resX;x++)
    {
      if (frame !=0)
        if (x % frame == 0)
          image.pixels[offset + x].set(1.0, 0.0, 0.0, 0.0);
    }
  }
  */
  for (UInt32 x=0;x<resX;x++) {
    if ((ls_position + x) < sample0.size()) {
      // Channel 0
      UInt32 y = -sample0[ls_position + x]*100.0+128;
      UInt32 idx = y *resX + x;
      image.pixels[idx].set(1.0, 1.0, 1.0, 0.0);
      // Channel 1
      y = -sample1[ls_position + x]*100.0+384;
      idx = y *resX + x;
      image.pixels[idx].set(1.0, 1.0, 1.0, 0.0);
    }
  }
}
   
   "
        }
