        "extDeps" : {},
        "code" : "require Math;
require OpenImageIO;
require Images;
require Singletons;

dfgEntry
{ 
  UInt32 resX = 512;
  UInt32 resY = 256;
  
  // Init or retrieve image
  if(Singleton_has('spectrumimage')) {
    image = Singleton_get('spectrumimage');
  } else {
    Image2DColor image_obj = Image2DColor();
    Singleton_set('spectrumimage', image_obj);
    image_obj.resize(resX, resY);
    for (UInt32 i=0;i<image_obj.pixels.size();i++)
      image_obj.pixels[i].set(0.0, 0.0, 0.0, 0.0);
    image = image_obj;
  }

  // draw spectrum  
  for (UInt32 x=0;x<resX/2;x++) {
    Float32 val0 = spectrum0[x]/20.0;
    Float32 val1 = spectrum1[x]/20.0;
    image.pixels[255*resX + 255-x].set(val0, 0, 0, 0.0);
    image.pixels[255*resX + x+256].set(val1, 0, 0, 0.0); //256 is resX/2
  }
  
  // scroll image
  for (UInt32 y=0;y<resY-1;y++) {
    for (UInt32 x=0;x<resX;x++) {
      UInt32 idx_from = (y+1)*resX + x;
      UInt32 idx_to = (y)*resX + x;
      image.pixels[idx_to] = image.pixels[idx_from];
    }
  }
  
}
   
   "
